name: WPS会员
desc: WPS解锁本地会员,支持大陆版以及非大陆版 @R·E https://t.me/Functional_Store_Hub

http:
  mitm:
    - 'account.wps.cn'
    - '*account.wps.com'
  script:
    - match: ^https?:\/\/.*?account\.wps\.(com|cn)(:\d+)?\/api\/users\/\w+\/overview$
      name: WPS.response
      type: response
      require-body: true
      timeout: 10
      
script-providers:
  WPS.response:
    url: https://cdn.jsdelivr.net/gh/Akimio521/BetterRuler@main/Script/WPS.js
    interval: 86400
